{
  "name": "@immowelt/jest-preset-node",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description":
    "Common configuration options for Jest in Node.js environments.",
  "main": "jest-preset.json",
  "scripts": {
    "test": "yarn run lint",
    "lint": "eslint .",
    "semantic-release":
      "semantic-release pre && npm publish --access=public && semantic-release post",
    "precommit": "lint-staged && npm test"
  },
  "lint-staged": {
    "**/*.{js,json,css}": ["prettier --write", "git add"]
  },
  "engines": {
    "node": ">=8.2.0"
  },
  "devDependencies": {
    "@immowelt/eslint-config-immowelt-react": "2.0.4",
    "@immowelt/styleguide-javascript": "1.0.11",
    "@inkdpixels/commit-analyzer": "1.0.1",
    "@inkdpixels/release-notes-generator": "1.0.1",
    "eslint": "4.19.1",
    "husky": "0.14.3",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2",
    "semantic-release": "^8.0.0"
  },
  "dependencies": {
    "babel-jest": "^22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/jest-preset-node.git"
  },
  "release": {
    "analyzeCommits": "@inkdpixels/commit-analyzer",
    "generateNotes": "@inkdpixels/release-notes-generator"
  }
}
