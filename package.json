{
  "name": "@immowelt/jest-preset-node",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description": "Common configuration options for Jest in Node.js environments.",
  "main": "jest-preset.json",
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/jest-preset-node.git"
  },
  "scripts": {
    "release": "semantic-release"
  },
  "files": [
    "jest-preset.json"
  ],
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "devDependencies": {
    "@semantic-release/github": "5.0.5",
    "@semantic-release/npm": "5.0.4",
    "semantic-release": "15.9.15"
  },
  "peerDependencies": {
    "jest": "^23.0.0"
  },
  "keywords": [
    "immowelt",
    "jest",
    "preset",
    "node"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branch": "master",
    "analyzeCommits": {
      "releaseRules": [
        {
          "type": "BREAKING",
          "release": "major"
        },
        {
          "type": "FEATURE",
          "release": "minor"
        },
        {
          "type": "BUGFIX",
          "release": "patch"
        },
        {
          "type": "SECURITY",
          "release": "patch"
        }
      ]
    }
  }
}
